    "# Initialize an empty list to store the integers\n",
    "numbers = []\n",
    "\n",
    "# Loop to take 10 integer inputs from the user\n",
    "for i in range(10):\n",
    "    num = int(input(f\"Enter integer #{i + 1}: \"))\n",
    "    numbers.append(num)\n",
    "\n",
    "# Display the list of numbers\n",
    "numbers\n"
   ]
